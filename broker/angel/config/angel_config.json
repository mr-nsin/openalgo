      "close": "23:30:00"
    }
  },
  "ui_config": {
    "theme_color": "#FF6B35",
    "logo_url": "/static/images/brokers/angel.png",
    "login_redirect_url": "/angel/callback",
    "documentation_url": "https://smartapi.angelbroking.com/docs",
    "support_url": "https://www.angelone.in/support",
    "broker_website": "https://www.angelone.in"
  },
  "version": "1.0.0",
